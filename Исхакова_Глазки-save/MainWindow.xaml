<Window x:Class="Исхакова_Глазки_save.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Исхакова_Глазки_save"
        mc:Ignorable="d"
        Title="Исхакова З.И." Height="450" Width="800" FontFamily="Century Gothic"> 
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75" />
            <RowDefinition Height="*" />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <Image HorizontalAlignment="Left"></Image>
        <TextBlock Grid.Row="0" Text="Глазки-save" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack"  Width="175" Height="30" Margin="5" HorizontalAlignment="Right" Click="BtnBack_Click"  ></Button>
    </Grid>
</Window>
